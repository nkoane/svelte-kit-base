{
    "name": "base",
    "version": "0.0.2",
    "type": "module",
    "author": "lebogang nkoane",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+github.com/nkoane/svelte-kit-base"
    },
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
